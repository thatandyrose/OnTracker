= form_for @status do |f|
  = render "shared/error_messages", target: @status
  .form-group
    = f.label :key
    = f.text_field :key, disabled: true, class: 'form-control'
  .form-group
    = f.label :label
    = f.text_field :label, class: 'form-control'
  = f.submit 'Save status', class: 'btn btn-lg btn-primary'